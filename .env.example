# Required: AWS Prefix List ID
PREFIX_LIST_ID=pl-12345678abcdef

# AWS Credentials (if not using IAM role)
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
# AWS_SESSION_TOKEN=your_session_token_here  # Only needed for temporary credentials

# AWS Region
AWS_REGION=us-east-1

# Entry description (used to identify entries managed by this tool)
ENTRY_DESCRIPTION=Auto-updated host IP

# Check interval in seconds (default: 300 = 5 minutes)
CHECK_INTERVAL=300

# CIDR suffix (default: 32 for single host)
# Use 32 for IPv4 single host (/32)
# Use 24 for class C network (/24), etc.
CIDR_SUFFIX=32

# IP detection service URL
IP_SERVICE_URL=https://api.ipify.org

# Logging level (trace, debug, info, warn, error)
RUST_LOG=info